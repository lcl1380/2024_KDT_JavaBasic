package super_;

public class Airplane_Main {

	public static void main(String[] args) {
		Airplane ap1 = new Airplane("국내선");
		Supersonic_Airplane ap2 = new Supersonic_Airplane("외국선");
		
		for(int i=0; i < 10; i++) {
			ap1.fly();
			ap1.showStatus();
		}
		
		for(int i=0; i < 10; i++) {
			ap2.fly();
			ap2.showStatus();
		}
		
	}

}
