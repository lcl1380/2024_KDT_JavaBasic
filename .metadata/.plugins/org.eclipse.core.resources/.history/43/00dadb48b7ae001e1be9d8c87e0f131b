package no_inheritance;

public class MainClass1 {

	public static void main(String[] args) {
		// 전사 1 생성
		Warrior w1 = new Warrior("빛의 전사");
		// 생성 직후 정보 조회
		w1.show_Status();
		
		// 토끼 생성
		Rabbit r1 = new Rabbit();
		
		// 토끼와 교전
		w1.hunt_rabbit(r1);
		//죽은 토끼 한 번 더 공격해보기
		w1.hunt_rabbit(r1);
		// 사냥 후 정보 조회
		w1.show_Status();

	}

}
